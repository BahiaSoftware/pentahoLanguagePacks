      "name": "listeners",
      "value": "['${p:metadataParam}']",
      "type": "Listeners"
     },
     {
      "name": "expression",
      "value": "function (){\n\n    var json = Dashboards.getParameterValue('${p:metadataParam}');\n    \n    $content = $(this.template(json));\n\n    $('#' + this.htmlObject).prepend( $content );\n    \n    \n}",
      "type": "JavaScript"
     },
     {
      "name": "priority",
      "value": 5,
      "type": "Integer"
     },
