     {
      "name": "postFetch",
      "value": "function f(json){\n    \n    var json0 = {\n        \"appTitle\": \"{{language}} Language Pack Installer\",\n        \"appWelcomeMessage\": \"This plugin installs language definitions for the {{language}} language in Pentaho 5\",\n        \"language\": \"{{language}}\",\n        \"languageCode\": \"{{languageCode}}\",\n        \"version\": \"0.1\",\n        \"pentahoVersion\": \"5.0.1\",\n        \"maintainer\": {\n            \"name\": \"\",\n            \"email\": \"\",\n            \"img\": \"\",\n            \"url\": \"http:\\/\\/www.pentaho.com\"\n        },\n        \"credits\": [\n            {\n                \"name\": \"\",\n                \"email\": \"\",\n                \"img\": \"\",\n                \"url\": \"\"\n            }\n        ],\n        \"sponsors\": [\n            {\n                \"name\": \"\",\n                \"email\": \"\",\n                \"img\": \"\",\n                \"url\": \"\"\n            }\n        ],\n    \n    };\n    \n    Dashboards.fireChange('${p:metadataParam}', json);\n    \n    return json\n    \n}",
      "type": "JavaScript"
     },
